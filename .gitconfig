[ui]

[color]
	ui = auto

[alias]
	all = log --oneline --decorate --graph --all
	ll = log --oneline --decorate --graph
	b = branch
	o = checkout
	t = status
	d = diff
	i = commit
	a = add
	r = rebase
	l = log --decorate
	il = log --decorate
	g = grep
	cp = cherry-pick
	sr = svn rebase
	sd = svn dcommit
	p = pull

[user]
	name = Ramkumar Aiyengar
	mail = andyetitmoves@gmail.com

[user "https://git-wip-us.apache.org"]
	name = Ramkumar Aiyengar
	mail = andyetitmoves@apache.org

[user "https://bbgithub.dev.bloomberg.com"]
	name = Ramkumar Aiyengar
	mail = raiyengar@bloomberg.net

[branch]
	autosetuprebase = always

[push]
	default = simple

[merge]
	conflictstyle = diff3

[hub]
	protocol = https

[credential]
	helper = osxkeychain

[http]
	proxy = http://localhost:8888
	sslCAinfo = ~/.bbcert/bloomberg-root-ca.crt

[url "bbgithub:"]
	insteadOf = https://bbgithub.dev.bloomberg.com

[url "https://github.com/"]
	insteadOf = github.com:
[url "https://github.com/"]
	insteadOf = git@github.com:
[url "https://github.com/"]
	insteadOf = ssh://git@github.com/
[url "https://gitlab.com/"]
	insteadOf = gitlab.com:
[url "https://gitlab.com/"]
	insteadOf = git@gitlab.com:
[url "https://gitlab.com/"]
	insteadOf = ssh://git@gitlab.com/
